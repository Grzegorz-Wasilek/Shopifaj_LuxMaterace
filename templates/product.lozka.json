    "custom_html_Ni3Nmn": {
      "type": "custom-html",
      "disabled": true,
      "custom_css": [
        "button.prd-btn {cursor: pointer; background: transparent; padding: 1rem 1rem; border-radius: 100px; box-shadow: none; border: solid 1px #bfccd9; font-size: 1rem; font-weight: 700; color: #022e4f; -webkit-transition: all 0.3s cubic-bezier(0.165, 0.84, 0.44, 1); -moz-transition: all 0.3s cubic-bezier(0.165, 0.84, 0.44, 1); -o-transition: all 0.3s cubic-bezier(0.165, 0.84, 0.44, 1); transition: all 0.3s cubic-bezier(0.165, 0.84, 0.44, 1); margin: 0 15px;}",
        "button.prd-btn:hover,button.prd-btn:active {background: #ed979c; border-color: #ed979c;}"
      ],
      "settings": {
        "full_width": true,
        "remove_vertical_spacing": false,
        "remove_horizontal_spacing": false,
        "html": "<div class=\"flex-wrap\">\n<button class=\"prd-btn desc-mat-prd\">Opis<\/button>\n  <button class=\"prd-btn cons-mat-prd\">Budowa<\/button>\n  <button class=\"prd-btn komf-mat-prd\">Strefy komfortu<\/button>\n  <button class=\"prd-btn mat-mat-prd\">Materiały<\/button>\n  <button class=\"prd-btn cover-mat-prd\">Pokrowce<\/button>\n <\/div>\n<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n    function scrollToElement(buttonSelector, targetIdContains) {\n        document.querySelector(buttonSelector).addEventListener('click', function() {\n          \n            const element = document.querySelector(`[id*='${targetIdContains}']`);\n            if (element) {\n                element.scrollIntoView({\n                    behavior: 'smooth'\n                });\n            }\n        });\n    }\n\n   \n    scrollToElement('.desc-mat-prd', 'media_with_text');\n    scrollToElement('.cons-mat-prd', 'hot_spots');\n    scrollToElement('.komf-mat-prd', 'image_with_text_overlay');\n    scrollToElement('.mat-mat-prd', 'material-section-in-btn');\n    scrollToElement('.cover-mat-prd', 'cover-section-in-btn');\n});\n<\/script>\n\n<style>\n.flex-wrap {\ndisplay: flex;\nflex-wrap: wrap;\njustify-content: center;\nalign-items: stretch;\ngap: 1rem;  \nalign-items: center;\npadding-top: 2.75rem;\npadding-left: 6rem;\npadding-right: 6rem;\n}\n\n.prd-btn {\n  display: block;\n  flex-grow: 1;\n  flex-shrink:1;\n  text-align: center;\n  flex: 15%;  \n}\n@media (max-width: 420px) {\n.prd-btn {\n flex: 100%;\n}\n}\n<\/style>",
        "background": "",
        "background_gradient": "",
        "text_color": ""
      }
